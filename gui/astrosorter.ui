}
/*																QPushbutton																		*/	
#mainLayout &gt; QPushButton {
	font: 11pt &quot;Bahnschrift SemiBold&quot;;
	background: none;
	border: 2px solid rgba(80, 95, 105, 150);
 	border-radius: 2px;
	border: none;
	padding: 5px;
	border-radius: 3px;
}
#mainLayout &gt; #rectButton:hover:!disabled {
	padding-bottom: 7px;
	border: 1px solid rgb(0, 167, 209);
}
#directoryButton, #userButton, #sampleButton {
	background: none;
	border: none;
}
#directoryButton:hover:!disabled, #userButton:hover:!disabled, #sampleButton:hover:!disabled {
	border: 1px solid rgb(0, 167, 209);
	padding-bottom: 7px;
} 
QPushButton {
	font: 11pt &quot;Bahnschrift SemiBold&quot;;
	background-color: qlineargradient(x1: 0.5, y1: 1.0, 
		x2: 0.5, y2: 0.0,
		stop: 1.0 rgb(68, 86, 100), 
		stop: 0.0 rgb(44, 55, 64));
	border: 1px solid rgba(80, 95, 105, 255);
	padding: 5px;
	border-radius: 3px;
}
QPushButton:hover:!disabled {
	background: qlineargradient(x1: 0.5, y1: 1.0, 
		x2: 0.5, y2: 0.0,
		stop: 1.0 rgb(68, 86, 100), 
		stop: 0.0 rgb(44, 55, 64));
	padding-bottom: 7px;
	border: 1px solid rgb(0, 167, 209);
}
#mainLayout &gt; QPushButton:hover:!disabled {
	border: 1px solid rgb(0, 167, 209);
	padding: 5px 5px 7px 5px;
}
QPushButton::disabled {
	color: rgb(68, 86, 100);
	background-color: qlineargradient(x1: 0.5, y1: 1.0, 
		x2: 0.5, y2: 0.0,
		stop: 1.0 rgb(34, 43, 53), 
		stop: 0.0 rgb(44, 55, 64));
}
#startstopwatchButton {
	color: rgb(155, 229, 100);
}
#resetstopwatchButton::enabled {
	color: rgb(228, 88, 101);
}
#starttimerButton::enabled {
	color: rgb(155, 229, 100);
}
#starttimerButton::disabled:checked {
	color: rgb(228, 88, 101);
}
#resettimerButton::enabled {
	color: rgb(228, 88, 101);
}
