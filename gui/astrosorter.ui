}
/*																QScrollBar																			*/
QScrollBar {
	background: transparent;
	border: 1px solid rgba(111, 124, 132, 150);
	border-radius: 5px;
	width: 16px;
	height: 16px;
	margin: 3px;
}
QScrollBar:vertical {
	margin-top: 13px;
	margin-bottom: 13px;
}
QScrollBar:horizontal {
	margin-left: 13px;
	margin-right: 13px;
}
QScrollBar::sub-line:vertical {
	subcontrol-origin: border;
	subcontrol-position: top;
	background: none;
	border-radius: 3px;
	height: 13px;
	top: -13px;
}
QScrollBar::add-line:vertical {
	subcontrol-origin: border;
	subcontrol-position: bottom;
	background: none;
	border-radius: 3px;
	height: 13px;
	bottom: -13px;
}
QScrollBar::sub-line:horizontal {
	subcontrol-origin: border;
	subcontrol-position: left;
	background: none;
	border-radius: 3px;
	width: 13px;
	left: -13px;
}
QScrollBar::add-line:horizontal {
	subcontrol-origin: border;
	subcontrol-position: right;
	background: none;
	border-radius: 3px;
	width: 13px;
	right: -13px;
}
QScrollBar::up-arrow:vertical {
	subcontrol-origin: border;
	subcontrol-position: top;
	image: url(:/icons/icons/up button gray.png);
	width: 12px;
}
QScrollBar::up-arrow:vertical:hover {
	image: url(:/icons/icons/up button hover.png);
}
QScrollBar::down-arrow:vertical {
	subcontrol-origin: border;
	subcontrol-position: bottom;
	image: url(:/icons/icons/down button gray.png);
	width: 12px;
}
QScrollBar::down-arrow:vertical:hover {
	image: url(:/icons/icons/down button hover.png);
}
QScrollBar::left-arrow:horizontal {
	subcontrol-origin: border;
	subcontrol-position: left;
	image: url(:/icons/icons/left button gray.png);
	height: 12px;
}
QScrollBar::left-arrow:horizontal:hover {
	image: url(:/icons/icons/left button hover.png);
}
QScrollBar::right-arrow:horizontal {
	subcontrol-origin: border;
	subcontrol-position: right;
	image: url(:/icons/icons/right button gray.png);
	height: 12px;
}
QScrollBar::right-arrow:horizontal:hover {
	image: url(:/icons/icons/right button hover.png);
}
QScrollBar::handle {
	background-color: rgba(80, 95, 105, 150);
	border: 1px solid rgb(111, 124, 132);
	border-radius: 3px;
}
QScrollBar::handle:hover {
	border: 1px solid rgb(0, 167, 209);
	background-color: rgb(0, 167, 209);
}
QScrollBar::add-page, QScrollBar::sub-page {
	background: none;
}
/*																QSlider																					*/	
QSlider::groove {
	height: 6px;
	border: 0px solid rgb(25, 35, 45);
