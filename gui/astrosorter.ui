/*																QTabBar																				*/	
QTabBar::tab {
	font: 11pt &quot;Bahnschrift SemiBold&quot;;
	color: white;
	border: 1px solid rgb(68, 86, 100);
	border-bottom-width: 0px;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	padding: 2px 10px 5px 10px;
	margin-right: 3px;
	background: qlineargradient(spread:pad, 
		x1:1, y1:1, 
		x2:1, y2:0.585227, 
		stop:0 rgba(44, 55, 64, 255), 
		stop:1 rgba(80, 95, 105, 255));
}
QTabBar::tab:hover {
	border: 1px solid rgb(0, 167, 209);
	background: qlineargradient(x1: 0.5, y1: 0.86,
		x2: 0.5, y2: 1.0,
 		stop: 0.0 rgb(50, 65, 75),
		stop: 1.0  rgb(0, 167, 209));
	background: qlineargradient(spread:pad, x1:1, y1:0.977273, x2:1, y2:0, stop:0 rgba(0, 167, 209, 156), stop:0.198864 rgba(80, 95, 105, 255));
	padding: 2px 10px 6px 10px;
}
QTabBar::tab:selected {
	border: none;
	background: rgb(68, 86, 100);
	border-bottom: 1px solid rgb(0, 167, 209);
	border-top: 1px solid rgb(68, 86, 100);
	padding: 2px 11px 5px 11px;
	background: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0.83, stop:0 rgba(0, 167, 209, 255), stop:1 rgba(80, 95, 105, 255));
}
QTabBar::tab:selected:hover {
	border: 1px solid rgb(0, 167, 209);
	background: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0.779, stop:0 rgba(0, 167, 209, 255), stop:1 rgba(80, 95, 105, 255));
	padding: 2px 10px 7px 10px;
}
QTabBar::scroller {
	width: 16px;
}
QTabBar QToolButton {
	background-color: rgba(25, 35, 45, 220);
	border: none;
	border-radius: 3px;
}
QTabBar QToolButton::left-arrow {
	image: url(:/icons/icons/left button.png);
}
QTabBar QToolButton::left-arrow:disabled {
	image: url(:/icons/icons/left button hover.png);
}
QTabBar QToolButton::left-arrow:disabled {
	image: url(:/icons/icons/left button gray.png)
}
QTabBar QToolButton::right-arrow {
	image: url(:/icons/icons/right button.png);
}
QTabBar QToolButton::right-arrow:hover {
	image: url(:/icons/icons/right button hover.png);
}
QTabBar QToolButton::right-arrow:disabled {
	image: url(:/icons/icons/right button gray.png)
}
QTabBar::close-button {
	image: url(:/icons/icons/close tab icon.png);
	subcontrol-position: right;
}
QTabBar::close-button:hover {
	image: url(:/icons/icons/close tab hover icon.png);
}
/*																QTabWidget																		*/	
QTabWidget::pane {
	border: 1px solid rgb(80, 95, 105);
	border-radius: 3px;
	border-top-left-radius: 0px;
}
QTabWidget::pane:hover {
	border: 1px solid rgb(0, 167, 209);
}
/*																QStatusBar																			*/	
QStatusBar::item {
